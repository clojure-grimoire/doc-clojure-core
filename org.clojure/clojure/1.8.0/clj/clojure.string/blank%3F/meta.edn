{:added "1.2", :ns "clojure.string", :name "blank?", :file "clojure/string.clj", :type :fn, :src "(defn blank?\n  \"True if s is nil, empty, or contains only whitespace.\"\n  {:added \"1.2\"}\n  [^CharSequence s]\n  (if s\n    (loop [index (int 0)]\n      (if (= (.length s) index)\n        true\n        (if (Character/isWhitespace (.charAt s index))\n          (recur (inc index))\n          false)))\n    true))", :column 1, :line 287, :arglists ([s]), :doc "True if s is nil, empty, or contains only whitespace."}