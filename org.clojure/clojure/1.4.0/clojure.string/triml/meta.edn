{:arglists ([s]), :ns "clojure.string", :name "triml", :src "(defn ^String triml\n  \"Removes whitespace from the left side of string.\"\n  {:added \"1.2\"}\n  [^CharSequence s]\n  (loop [index (int 0)]\n    (if (= (.length s) index)\n      \"\"\n      (if (Character/isWhitespace (.charAt s index))\n        (recur (inc index))\n        (.. s (subSequence index (.length s)) toString)))))", :added "1.2", :type :fn, :doc "Removes whitespace from the left side of string.", :line 190, :file "clojure/string.clj", :tag java.lang.String}