{:added "1.0", :ns "clojure.core", :name "<", :file "clojure/core.clj", :type :fn, :src "(defn <\n  \"Returns non-nil if nums are in monotonically increasing order,\n  otherwise false.\"\n  {:inline (fn [x y] `(. clojure.lang.Numbers (lt ~x ~y)))\n   :inline-arities #{2}\n   :added \"1.0\"}\n  ([x] true)\n  ([x y] (. clojure.lang.Numbers (lt x y)))\n  ([x y & more]\n   (if (< x y)\n     (if (next more)\n       (recur y (first more) (next more))\n       (< y (first more)))\n     false)))", :inline-arities #{2}, :column 1, :line 859, :arglists ([x] [x y] [x y & more]), :doc "Returns non-nil if nums are in monotonically increasing order,\n  otherwise false."}